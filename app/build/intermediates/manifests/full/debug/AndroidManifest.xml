<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.uli2.userprofilemgmt"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="26" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme1" >
        <activity android:name="com.example.uli2.userprofilemgmt.SplashScreen" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.uli2.userprofilemgmt.MainActivity" >
        </activity>
        <activity
            android:name="com.example.uli2.userprofilemgmt.ApplicationActivity"
            android:theme="@style/AppThemeToolBar" >
        </activity>
        <activity
            android:name="com.example.uli2.userprofilemgmt.TopApplicationActivity"
            android:theme="@style/AppThemeToolBar" >
        </activity>
        <activity
            android:name="com.example.uli2.userprofilemgmt.TopUserActivity"
            android:theme="@style/AppThemeToolBar" >
        </activity>
        <activity android:name="com.example.uli2.userprofilemgmt.AppRecyclerView.CActivity" >
        </activity>
        <activity
            android:name="com.example.uli2.userprofilemgmt.VisitorActivity"
            android:theme="@style/AppThemeToolBar" >
        </activity>

        <meta-data
            android:name="android.support.VERSION"
            android:value="26.0.0" />

        <provider
            android:name="android.arch.lifecycle.LifecycleRuntimeTrojanProvider"
            android:authorities="com.example.uli2.userprofilemgmt.lifecycle-trojan"
            android:exported="false"
            android:multiprocess="true" />
    </application>

</manifest>